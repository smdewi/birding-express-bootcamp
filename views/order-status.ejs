<h1>Order: <%= order.id %></h1>
<table>
  <tr>
    <th>From</th>
    <th>Patient ID</th>
    <th>Order Date</th>
    <th>Order Status</th>
  </tr>
  
  <tr>
    <td><%= order.customer_id %></td>
    <td><%= order.patient_id %></td>
    <td><%= order.order_date %></td>
    <td><%= order.order_status %></td>
  </tr>
  
</table>

<form>
  <input type="submit" value="Accept" />
</form>
<form>
  <input type="submit" value="Reject" />
</form>